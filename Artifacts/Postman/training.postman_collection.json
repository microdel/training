					"event": [
						{
							"listen": "test",
							"script": {
								"type": "text/javascript",
								"exec": [
									"var jsonData = JSON.parse(responseBody);",
									"if (jsonData.token) {",
									"    postman.setEnvironmentVariable(\"token\", jsonData.token);",
									"    tests['login'] = true",
									"}",
									"else {",
									"    tests['login'] = false",
									"}"
								]
							}
						}
					],
					"request": {
				"url": "{{host}}/api/auth",
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
								"description": ""
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n   \"email\": \"user@mail.com\",\n   \"password\": \"123456\"\n}"
