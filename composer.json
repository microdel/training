{
  "name": "saritasa/training",
  "description": "Training",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1",
    "barryvdh/laravel-cors": "^0.11",
    "dingo/api": "^2.0@alpha",
    "laravel/framework": "5.5.*",
    "laravel/tinker": "^1.0",
    "laravelcollective/html": "5.5.*",
    "predis/predis": "^1.1",
    "saritasa/blade-directives": "^1.0",
    "saritasa/dingo-api-custom": "^2.0",
    "saritasa/laravel-controllers": "^2.0",
    "saritasa/laravel-notifications-api": "^1.0",
    "saritasa/laravel-middleware": "^1.0",
    "saritasa/roles-simple": "^1.0",
    "tymon/jwt-auth": "1.0.*@dev"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.1",
    "barryvdh/laravel-ide-helper": "^2.4",
    "doctrine/dbal": "^2.6",
    "fzaninotto/faker": "^1.7",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^6.5",
    "reliese/laravel": "^0.0.13",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php tools/install-git-hooks.php"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php tools/install-git-hooks.php"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "prefer-stable": true
}
